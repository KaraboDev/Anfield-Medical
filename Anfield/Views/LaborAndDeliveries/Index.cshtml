@model IEnumerable<Anfield.Models.LaborAndDelivery>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.LaborStartDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeliveryDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeliveryMethod)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Patient)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.LaborStartDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DeliveryDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DeliveryMethod)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Patient.ContactNumber)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.LaborAndDeliveryId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.LaborAndDeliveryId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.LaborAndDeliveryId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
